<h2>Hello !</h2>

<h3>My artists:</h3>

<p>

  <% @organization.artists.each do |artist| %>
  	<li><%= link_to "#{artist.name}", organization_artist_path(@organization, artist)  %></li>
  <% end %>
</p>

<h3>My artworks:</h3>

<p>
	<% @organization.artworks.each do |artwork| %>
		<li><%= link_to "#{artwork.title}", organization_artist_artwork_path(@organization, artwork.artist_id, artwork.id) %>  </li>
	<% end %>

</p>

<h3>My customers:</h3>

<p>
	<% @organization.customers.each do |customer| %>
		<li><%= link_to "#{customer.name}", organization_customer_path(@organization, customer.id) %>  </li>
	<% end %>

</p>

<p> <%= link_to "Create a new customer", new_organization_customer_path(@organization) %> </p>